<p id="notice"><%= notice %></p>

<h1>Store Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Datetime</th>
      <th>Charge</th>
      <th>Customerid</th>
      <th>Productid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @store_transactions.each do |store_transaction| %>
      <tr>
        <td><%= store_transaction.dateTime %></td>
        <td><%= store_transaction.charge %></td>
        <td><%= store_transaction.customerID %></td>
        <td><%= store_transaction.productID %></td>
        <td><%= link_to 'Show', store_transaction %></td>
        <td><%= link_to 'Edit', edit_store_transaction_path(store_transaction) %></td>
        <td><%= link_to 'Destroy', store_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store Transaction', new_store_transaction_path %>
